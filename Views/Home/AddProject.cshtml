						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="BackendDevelopmentPhaseStart" class="date-picker form-control" placeholder="Start" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="BackendDevelopmentPhaseStart" class="text-danger"></span>

						</div>

						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="BackendDevelopmentPhaseFinish" class="date-picker form-control" placeholder="Finish" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="BackendDevelopmentPhaseFinish" class="text-danger"></span>
						</div>

					</div>

					@*FronendDevelopment*@
					<div class="item form-group">
						<label class="col-form-label col-md-3 col-sm-3 label-align">
							FronendDevelopment TimeLine <span class="required">*</span>
						</label>
						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="FronendDevelopmentPhaseStart" class="date-picker form-control" placeholder="Start" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="FronendDevelopmentPhaseStart" class="text-danger"></span>

						</div>

						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="FronendDevelopmentPhaseFinish" class="date-picker form-control" placeholder="Finish" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="FronendDevelopmentPhaseFinish" class="text-danger"></span>
						</div>

					</div>

					@*Testing*@
					<div class="item form-group">
						<label class="col-form-label col-md-3 col-sm-3 label-align">
							Testing TimeLine <span class="required">*</span>
						</label>
						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="TestingPhaseStart" class="date-picker form-control" placeholder="Start" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="TestingPhaseStart" class="text-danger"></span>

