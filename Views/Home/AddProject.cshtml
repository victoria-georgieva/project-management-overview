							<span asp-validation-for="TestingPhaseFinish" class="text-danger"></span>
						</div>

					</div>


					@*UAT*@
					<div class="item form-group">
						<label class="col-form-label col-md-3 col-sm-3 label-align">
							UAT TimeLine <span class="required">*</span>
						</label>
						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="UATPhaseStart" class="date-picker form-control" placeholder="Start" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="UATPhaseStart" class="text-danger"></span>

						</div>

						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="UATPhaseFinish" class="date-picker form-control" placeholder="Finish" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="UATPhaseFinish" class="text-danger"></span>
						</div>

					</div>


					@*Release*@
					<div class="item form-group">
						<label class="col-form-label col-md-3 col-sm-3 label-align">
							Release TimeLine <span class="required">*</span>
						</label>
						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="ReleasePhaseStart" class="date-picker form-control" placeholder="Start" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="ReleasePhaseStart" class="text-danger"></span>

						</div>

						<div class="col-md-3 col-sm-3">
							<input id="birthday" asp-for="ReleasePhaseFinish" class="date-picker form-control" placeholder="Finish" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
							<span asp-validation-for="ReleasePhaseFinish" class="text-danger"></span>
						</div>

					</div>



					<div class="ln_solid"></div>
						<div class="item form-group">
							<div class="col-md-6 col-sm-6 offset-md-3">
								<button class="btn btn-primary" type="button">Cancel</button>
								<button type="submit" class="btn btn-success">Submit</button>
							</div>
						</div>

					</form>
				</div>
			</div>
		</div>
	</div>

